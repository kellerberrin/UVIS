#!/bin/bash
# Convenience script to download Drug Images and RxCui from the Nation Library of Medicine RxImage and RxNorm API
# All processing logic is in the Python file.

echo "UVISUSDRUGDATAFILE:" $UVISUSDRUGDATAFILE
echo "UVISUSDRUGIMAGELOGFILE:" $UVISUSDRUGIMAGELOGFILE
echo "UVISUSDRUGIMAGEDATAFILE:" $UVISUSDRUGIMAGEDATAFILE
echo "UVISUSDRUGIMAGECHECKPOINT:" $UVISUSDRUGIMAGECHECKPOINT
echo "UVISUSDRUGIMAGEMAXRATE:" $UVISUSDRUGIMAGEMAXRATE
echo "UVISUSDRUGIMAGEURL:" $UVISUSDRUGIMAGEURL
echo "UVISUSDRUGRXCUIURL:" $UVISUSDRUGRXCUIURL

python $UVISUSDRUGIMAGEDIR/getrximages.py
