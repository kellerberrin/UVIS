#!/bin/bash
# Run a local version of the UVIS PharmCat web application on
# the local Google App Engine server.

echo "UVISPHARMCATLOCALPORT:" $UVISPHARMCATLOCALPORT
echo "UVISPHARMCATLOCALHOST:" $UVISPHARMCATLOCALHOST
echo "UVISPHARMCATLOCALSTORAGE:" $UVISPHARMCATLOCALSTORAGE
echo "UVISPHARMCATADMINPORT:" $UVISPHARMCATADMINPORT
echo "UVISPHARMCATGAEAPPPATH:" $UVISPHARMCATGAEAPPPATH


python $GOOGLEAPPBIN/dev_appserver.py \
  --port=$UVISPHARMCATLOCALPORT  \
  --host=$UVISPHARMCATLOCALHOST  \
  --storage_path=$UVISPHARMCATLOCALSTORAGE \
  --admin_port=$UVISPHARMCATADMINPORT \
  $UVISPHARMCATGAEAPPPATH
